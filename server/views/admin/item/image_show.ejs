<div
  class="tab-pane fade show active"
  id="image_show"
  role="tabpanel"
  aria-labelledby="home-tab"
>
  <!-- DataTales-->
  <div class="row">
    <% for (let i = 0; i < item.imageId.length; i++) { %>
    <div class="col-md-4">
      <div class="card shadow mb-4 mt-2">
        <div class="card-body">
          <img
            src="https://staycationadmin.herokuapp.com/<%=item.imageId[i].imageUrl %>"
            alt="image"
            style="width: 100%; display: block;"
          />
        </div>
      </div>
    </div>
    <% } %>
  </div>
  <a type="button" href="/admin/item" class="btn btn-warning">Back</a>
</div>
